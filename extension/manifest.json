{
    "manifest_version": 3,
    "name": "SkyVoice",
    "version": "1.0",
    "description": "Assistive Browser Agent for visually impaired users",
    "action": {
        "default_popup": "index.html",
        "default_title": "SkyVoice",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "permissions": [
        "activeTab"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}